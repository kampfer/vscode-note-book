{
    "name": "vscode-note-book",
    "displayName": "vscode-note-book",
    "description": "vscode-note-book",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:vscode-note-book.scan",
        "onCommand:vscode-note-book.preview"
    ],
    "main": "./src/extension.js",
    "contributes": {
        "markdown.previewScripts": [
            "./src/data.js",
            "./src/renderer.js"
        ],
        "markdown.markdownItPlugins": true,
        "commands": [
            {
                "command": "vscode-note-book.scan",
                "title": "vscode-note-book.scan"
            }
        ]
    },
    "devDependencies": {
        "@types/vscode": "^1.45.0",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.1.2",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "markdown-it": "^11.0.0",
        "markdown-it-codepen": "^1.0.1",
        "throttle-debounce": "^2.2.1"
    }
}
