{
    "name": "vscode-note-book",
    "displayName": "vscode-note-book",
    "description": "vscode-note-book",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:note-book",
        "onCommand:vscode-note-book.init",
        "onCommand:vscode-note-book.scanTags"
    ],
    "main": "./src/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vscode-note-book.init",
                "title": "init"
            },
            {
                "command": "vscode-note-book.scan",
                "title": "scan"
            }
        ],
        "views": {
            "explorer": [
                {
                    "id": "note-book",
                    "name": "标签"
                }
            ]
        }
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.45.0",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.1.2",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    }
}